{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/original-url/(.*)",
      "dest": "/app.js"
    },
    {
      "src": "/shortUrl",
      "dest": "/app.js"
    },
    {
      "src": "/([a-zA-Z0-9-_]{1,60})",
      "dest": "/app.js"
    },
    {
      "src": "/login",
      "dest": "/app.js"
    },
    {
      "src": "/logout",
      "dest": "/app.js"
    },
    {
      "src": "/callback",
      "dest": "/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}